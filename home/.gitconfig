[push]
	default = simple
[alias]
	s = status
    sh = show
	l = log --graph --decorate
	b = branch --color
	d = diff -w
	ds = diff --staged -w
	co = checkout
	ci = commit -v
	cia = commit --amend
	p = pull --prune
	pu = push
	puf = push --force
	forget = rm --cached
[include]
    path = .gitconfig.local
